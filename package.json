{
	"name": "ta3",
	"productName": "Transition Analysis 3",
	"version": "0.7.1",
	"description": "Transition Analysis 3 (TA3) Age Estimation",
	"license": "",
	"repository": "rer145/ta3",
	"author": {
		"name": "Ron Richardson",
		"email": "ron.richardson@gmail.com"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"test": "npm run lint",
		"start": "electron .",
		"start-analysis": "electron . --analysis=\"d:\\Test Path Windows\\TA 3\\scripts\"",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --windows -p always",
		"dist-win": "electron-builder --windows",
		"dist-mac": "electron-builder --macos",
		"travis-pack": "electron-builder --dir",
		"travis-dist": "electron-builder --macos --windows -p always",
		"travis-dist-win": "electron-builder --windows -p always",
		"travis-dist-mac": "electron-builder --macos -p always"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"bootstrap": "^3.3.7",
		"command-line-args": "^5.1.1",
		"electron-context-menu": "^0.15.0",
		"electron-debug": "^3.0.0",
		"electron-dl": "^2.0.0",
		"electron-log": "^4.1.1",
		"electron-pdf-window": "^1.0.12",
		"electron-store": "^4.0.0",
		"electron-unhandled": "^3.0.0",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.12.0",
		"electron-window-state": "^5.0.3",
		"find": "^0.3.0",
		"fs": "0.0.1-security",
		"jquery": "^3.3.1",
		"os": "^0.1.1",
		"performance-now": "^2.1.0",
		"popper.js": "^1.16.0",
		"sudo-prompt": "^9.0.0",
		"tether": "^1.4.7",
		"uuid": "^7.0.3"
	},
	"devDependencies": {
		"electron": "^6.0.7",
		"electron-builder": "^21.2.0"
	},
	"build": {
		"appId": "edu.psu.TA3",
		"productName": "Transition Analysis 3",
		"copyright": "Copyright ${author}",
		"directories": {
			"buildResources": "build",
			"output": "dist"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "rer145",
				"repo": "ta3"
			}
		],
		"win": {
			"icon": "build/icon.png",
			"verifyUpdateCodeSignature": false,
			"extraResources": [
				{
					"from": "./build/R-Portable/R-Portable-Win",
					"to": "R-Portable"
				},
				{
					"from": "./build/scripts",
					"to": "scripts"
				},
				{
					"from": "./build/setup",
					"to": "setup"
				},
				{
					"from": "./build/analyze.bat",
					"to": "analyze.bat"
				}
			]
		},
		"mac": {
			"category": "public.app-category.education",
			"darkModeSupport": false,
			"icon": "build/icon.icns",
			"extraResources": [
				{
					"from": "./build/R-Portable/R-Portable-Mac",
					"to": "R-Portable"
				},
				{
					"from": "./build/scripts",
					"to": "scripts"
				}
			]
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		}
	}
}
