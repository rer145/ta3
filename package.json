{
	"name": "ta3",
	"productName": "Transition Analysis 3",
	"version": "0.5.1",
	"description": "Transition Analysis 3 (TA3) Age Estimation",
	"license": "",
	"repository": "rer145/ta3-refactor",
	"author": {
		"name": "Ron Richardson",
		"email": "ron.richardson@gmail.com"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"dist-win": "electron-builder --windows",
		"dist-mac": "electron-buidler --macos",
		"release": "np"
	},
	"dependencies": {
		"bootstrap": "^3.3.7",
		"electron-context-menu": "^0.15.0",
		"electron-debug": "^3.0.0",
		"electron-store": "^4.0.0",
		"electron-unhandled": "^3.0.0",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.12.0",
		"electron-window-state": "^5.0.3",
		"find": "^0.3.0",
		"fs": "0.0.1-security",
		"jquery": "^3.3.1",
		"popper.js": "^1.16.0",
		"sudo-prompt": "^9.0.0",
		"tether": "^1.4.7"
	},
	"devDependencies": {
		"electron": "^6.0.7",
		"electron-builder": "^21.2.0",
		"np": "^5.0.3",
		"xo": "^0.24.0"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false,
		"tests": false
	},
	"build": {
		"appId": "edu.psu.TA3",
		"mac": {
			"category": "public.app-category.education",
			"darkModeSupport": true,
			"icon": "assets/img/icons/icon.png"
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Network;Chat"
		}
	}
}
